[Unit]
Description=ms-auth server
#Documentation=none
After=network.target

[Service]
Environment="SERVER_HOME={{.app.home}}/ms-auth"
ExecStart=/usr/local/bin/java -jar ${SERVER_HOME}/ms-auth.jar --spring.config.location=file:${SERVER_HOME}/bootstrap.yml
ExecStop=/bin/kill -9 $MAINPID
ExecReload=/bin/kill -HUP $MAINPID
Restart=on-failure

[Install]
Alias=ms-auth.service
WantedBy=multi-user.target